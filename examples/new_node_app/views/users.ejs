<% include partials/head.ejs %>
<body>
	<% include partials/header.ejs %>
	
	<!-- display our users -->
	<div id="users">
		<h2>Users</h2>
		<% let keys = Object.keys(data); %>
		<% keys.forEach(function(key) { %>
			<% const user = data[key]; %>
			<div class="user">
				<% if (user.profile_img_url) { %>
					<img src="<%= user.profile_img_url %>" width="100px">
				<% } %>
				<a href="/user/<%= key %>">
					<%= user.name %>
				</a>
			</div>
		<% }); %>
	</div>
	
	<% include partials/footer.ejs %>
</body>
</html>